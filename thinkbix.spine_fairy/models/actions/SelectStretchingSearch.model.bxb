action (SelectStretchingSearch) {
  description (장소,부위,상황을 입력받아 스트레칭을 찾음)
  type(Search)
  collect {
    input (place) {
      type (Place)
      min (Optional) max (One)
    }
    input (part) {
      type (Part)
      min (Optional) max (One)
    }
    input (situation) {
      type (Situation)
      min (Optional) max (One)
    }
  }
  output (StretchingSel)
}
