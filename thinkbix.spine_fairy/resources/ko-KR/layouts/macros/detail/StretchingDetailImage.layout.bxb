layout-macro-def(StretchingDetailImage) {
  params {
    param (processList) {
      type (ProcessList)
      min (Required)
      max (Many)
    }
  }

  content {
    section {  // this macro returns a "section"
      content {
        image {
          url ("#{value(processList.imageAddress)}")
          object-fit (Contain)
        }
      }
    }
  }
}